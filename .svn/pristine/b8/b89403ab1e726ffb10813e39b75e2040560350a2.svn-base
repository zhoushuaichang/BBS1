<?xml version="1.0" encoding="UTF-8" ?>
  <!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<package name="user" namespace="/" extends="struts-default,spring-default">
	<action name="reg" class="com.shinow.actions.RegLoginAction" method="regblog">
		<result>/index.jsp</result>
		<result name="input">/reg.jsp</result>
	</action>
	<action name="login" class="com.shinow.actions.RegLoginAction" method="checklogin">
		<result name="login">/index.jsp</result>
		<result name="input">/reg.jsp</result>

	</action>
	</package>
	
</struts>
